#!/bin/sh
#
# Purpose: resets contest status.
# Author : pc2@ecs.csus.edu
# $HeadURL$
#

. `dirname $0`/pc2env

if [ -d logs -a -d packets ] ; then
    `dirname $0`/pc2zip
    rm -fr logs packets reports db db.* execute*
else
    echo Not in directory to reset, should be run from dir with logs and packets
fi 


# eof pc2reset $Id$ 
