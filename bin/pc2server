#!/bin/sh
#
# Purpose: print version number
# Author : pc2@ecs.csus.edu
# $HeadURL: http://pc2.ecs.csus.edu/repos/pc2v9/trunk/bin/pc2team $
#

if [ -f ../VERSION ] ; then
  cd ..
fi 

java -cp lib\pc2.jar;lib\mclb.jar edu.csus.ecs.pc2.Starter --server $*

